<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">

    <ImageView
        android:id="@+id/retoureanmelden"
        android:onClick="OnClickRetoureAnmelden"

        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:clickable="true"
        app:srcCompat="@drawable/retoureanmelden"
        tools:layout_editor_absoluteY="60dp" />

    <ImageView
        android:id="@+id/meineretouren"
        app:layout_constraintTop_toBottomOf="@id/retoureanmelden"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/meineretouren"
        tools:layout_editor_absoluteY="20dp" />



</android.support.constraint.ConstraintLayout>