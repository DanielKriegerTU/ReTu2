<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">

    <include
        layout="@layout/action_bar"
        android:id="@+id/action_bar"/>

    <TextView
        android:id="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@id/action_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16pt"
        android:text="Wilkommen ZurÃ¼ck Daniel Krieger" />



    <ImageView
        app:layout_constraintTop_toBottomOf="@id/textView12"
        android:id="@+id/retoureanmelden"
        android:onClick="OnClickRetoureAnmelden"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:srcCompat="@drawable/retoureanmelden"
        />

    <ImageView
        android:id="@+id/meineretouren"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/retoureanmelden"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/meineretouren"
        tools:layout_editor_absoluteX="0dp" />



</android.support.constraint.ConstraintLayout>